      int arr[]= {10,20,50,40,90,65};
          int data = 100;
          int temp=0;
          for(int i =0;  i<arr.length;i++){
              
              if (arr[i]==data ){
                  
                  System.out.println("Element is prsent at" + i + " index position");
                  temp=temp+1;
              }
            
              }
                if(temp==0){
                  System.out.println("item is not found in list  ");
          }
          
    }
}
